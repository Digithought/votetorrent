{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
		"lib": ["ES2023"],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*", "../db-core/src/utility/ensured.ts"],
  "exclude": ["node_modules", "dist", "**/*.test.ts", "**/tests/**"]
}
